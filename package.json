{
  "name": "snake",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "bunx @biomejs/biome lint .",
    "lint.fix": "bunx @biomejs/biome lint . --apply",
    "format": "bunx @biomejs/biome format . --write && prettier --write \"*.{html,css}\"",
    "format.check": "bunx @biomejs/biome format . && prettier --check \"*.{html,css}\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "highcharts": "^11.4.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
